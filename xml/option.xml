<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.woltlab.com/XSD/maelstrom/option.xsd">
	<import>
		<categories>
			<category name="linklist">
			</category>
			<category name="dashboard.sidebar.linklist">
				<parent>dashboard.sidebar</parent>
			</category>
			<category name="linklist.general">
				<parent>linklist</parent>
			</category>
			<category name="linklist.general.settings">
				<parent>linklist.general</parent>
			</category>
			<category name="linklist.index">
				<parent>linklist</parent>
			</category>
			<category name="linklist.index.general">
				<parent>linklist.index</parent>
			</category>
			<category name="linklist.category">
				<parent>linklist</parent>
			</category>
			<category name="linklist.category.links">
				<parent>linklist.category</parent>
			</category>
			<category name="linklist.links">
				<parent>linklist</parent>
			</category>
			<category name="linklist.links.general">
				<parent>linklist.links</parent>
			</category>
		</categories>
		<options>
			<option name="module_linklist_comments">
				<categoryname>module.content</categoryname>
				<optiontype>boolean</optiontype>
				<defaulvalue>1</defaulvalue>
			</option>
			<option name="linklist_install_date">
				<categoryname>general</categoryname>
				<optiontype>integer</optiontype>
				<hidden>1</hidden>
			</option>
			<option name="linklist_users_online_record">
				<categoryname>general</categoryname>
				<optiontype>integer</optiontype>
				<hidden>1</hidden>
			</option>
			<option name="linklist_users_online_record_time">
				<categoryname>general</categoryname>
				<optiontype>integer</optiontype>
				<hidden>1</hidden>
			</option>
			<option name="linklist_latest_limit">
				<categoryname>dashboard.sidebar.linklist</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
			<option name="linklist_most_limit">
				<categoryname>dashboard.sidebar.linklist</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
			<option name="linklist_index_stats">
				<categoryname>linklist.index.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_index_wio">
				<categoryname>linklist.index.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_index_wio_noguests">
				<categoryname>linklist.index.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="linklist_index_wio_legend">
				<categoryname>linklist.index.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="linklist_enable_likes">
				<categoryname>linklist.links.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_enable_own_preview">
				<categoryname>linklist.links.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_enable_image_preview">
				<categoryname>linklist.links.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_enable_tags">
				<categoryname>linklist.links.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="linklist_links_per_page">
				<categoryname>linklist.category.links</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>20</defaultvalue>
			</option>
			<option name="linklist_default_sort_field">
				<categoryname>linklist.category.links</categoryname>
				<optiontype>select</optiontype>
				<defaultvalue>time</defaultvalue>
				<selectoptions>
          <![CDATA[subject:linklist.links.title
time:linklist.links.time
visits:linklist.links.visits]]>
				</selectoptions>
			</option>
			<option name="linklist_default_sort_order">
				<categoryname>linklist.category.links</categoryname>
				<optiontype>select</optiontype>
				<defaultvalue>DESC</defaultvalue>
				<selectoptions>
          <![CDATA[ASC:wcf.global.sortOrder.ascending
DESC:wcf.global.sortOrder.descending]]>
				</selectoptions>
			</option>
		</options>
	</import>
</data>