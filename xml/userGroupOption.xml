<?xml version="1.0" encoding="utf-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/userGroupOption.xsd">
	<import>
		<categories>
			<category name="user.linklist">
				<parent>user</parent>
			</category>
			<category name="user.linklist.category">
				<parent>user.linklist</parent>
			</category>
			<category name="user.linklist.link">
				<parent>user.linklist</parent>
			</category>
			<category name="mod.linklist">
				<parent>mod</parent>
			</category>
			<category name="mod.linklist.category">
				<parent>mod.linklist</parent>
			</category>
			<category name="mod.linklist.link">
				<parent>mod.linklist</parent>
			</category>
			<category name="admin.linklist">
				<parent>admin</parent>
			</category>
			<category name="admin.linklist.category">
				<parent>admin.linklist</parent>
			</category>
			<category name="admin.linklist.link">
				<parent>admin.linklist</parent>
			</category>
			<category name="admin.linklist.data">
				<parent>admin.linklist</parent>
			</category>
		</categories>

		<!-- Categories -->
		<options>
			<option name="admin.linklist.data.canImportData">
				<categoryname>admin.linklist.data</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="admin.linklist.category.canListCategory">
				<categoryname>admin.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="admin.linklist.category.canAddCategory">
				<categoryname>admin.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="admin.linklist.category.canDeleteCategory">
				<categoryname>admin.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="admin.linklist.category.canEditCategory">
				<categoryname>admin.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="user.linklist.category.canEnterCategory">
				<categoryname>user.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="user.linklist.category.canViewCategory">
				<categoryname>user.linklist.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<!--Link -->

			<option name="user.linklist.link.canViewLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canSeeDeactivatedLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="user.linklist.link.canAddLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canAddActiveLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canEditOwnLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canDeleteOwnLink">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canAddOwnPreview">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="mod.linklist.link.canDeleteLink">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="mod.linklist.link.canEditLink">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="mod.linklist.link.canTrashLink">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="mod.linklist.link.canToggleLink">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>

			<option name="user.linklist.link.canWriteComment">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canEditOwnComment">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="mod.linklist.link.canEditComment">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="mod.linklist.link.canModerateComment">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canDeleteOwnComment">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="mod.linklist.link.canDeleteComment">
				<categoryname>mod.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.canUploadAttachment">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.linklist.link.attachmentMaxSize,user.linklist.link.allowedAttachmentExtensions,user.linklist.link.maxAttachmentCount</enableoptions>
				<options>module_attachment</options>
			</option>
			<option name="user.linklist.link.attachmentMaxSize">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>fileSize</optiontype>
				<defaultvalue>1000000</defaultvalue>
				<options>module_attachment</options>
				<minvalue>10000</minvalue>
			</option>

			<option name="user.linklist.link.allowedAttachmentExtensions">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue>
          <![CDATA[gif
jpg
jpeg
png
bmp
zip
txt
pdf]]>
				</defaultvalue>
				<options>module_attachment</options>
				<wildcard><![CDATA[*]]></wildcard>
			</option>
			<option name="user.linklist.link.canDownloadAttachments">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.linklist.link.maxAttachmentCount">
				<categoryname>user.linklist.link</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
				<options>module_attachment</options>
				<minvalue>1</minvalue>
				<maxvalue>100</maxvalue>
			</option>
		</options>
	</import>
</data>